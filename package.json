{
  "name": "invoke-filter",
  "description": "An optimization for calling pure functions in the template of an Angular application",
  "version": "0.0.0-semantically-released",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:production & npm run build:development",
    "build:development": "NODE_ENV=development webpack",
    "build:production": "NODE_ENV=production webpack",
    "start": "NODE_ENV=test COVERAGE=true karma start",
    "test": "NODE_ENV=test COVERAGE=true karma start --single-run",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "echo 'Reporting coverage stats' && cat ./coverage/lcov.info | codecov",
    "commit": "git-cz",
    "prepublish": "npm run build",
    "postpublish": "publish-latest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/invoke-filter.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "filter",
    "invoke"
  ],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentcdodds/invoke-filter/issues"
  },
  "homepage": "https://github.com/kentcdodds/invoke-filter#readme",
  "devDependencies": {
    "angular": "1.3.18",
    "angular-mocks": "1.3.18",
    "babel": "5.8.21",
    "babel-core": "5.8.22",
    "babel-loader": "5.3.2",
    "chai": "3.2.0",
    "codecov.io": "0.1.6",
    "commitizen": "1.0.5",
    "cz-conventional-changelog": "1.1.0",
    "eslint": "1.2.0",
    "eslint-loader": "1.0.0",
    "eslint-plugin-mocha": "0.5.1",
    "ghooks": "0.3.2",
    "isparta": "3.0.3",
    "isparta-loader": "0.2.0",
    "istanbul": "0.3.18",
    "karma": "0.13.9",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.0",
    "karma-coverage": "0.5.0",
    "karma-firefox-launcher": "0.1.6",
    "karma-mocha": "0.2.0",
    "karma-sinon": "1.0.4",
    "karma-sinon-chai": "1.0.0",
    "karma-webpack": "1.7.0",
    "lodash": "3.10.1",
    "mocha": "2.2.5",
    "ng-annotate": "1.0.2",
    "ng-annotate-loader": "0.0.6",
    "path-here": "1.1.0",
    "publish-latest": "1.1.2",
    "semantic-release": "4.3.5",
    "sinon": "1.16.0",
    "sinon-chai": "2.8.0",
    "webpack": "1.11.0",
    "webpack-notifier": "1.2.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage && echo 'pre-commit checks good üëç'"
    }
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog/"
  }
}
